{
    "git.autofetch": true,
    "workbench.iconTheme": "material-icon-theme",
    "settingsSync.ignoredExtensions": [],
    "win-ca.inject": "append",
    "terminal.integrated.defaultProfile.windows": "PowerShell",
    "terminal.integrated.profiles.windows": {
        "PowerShell": {
            "source": "PowerShell",
            "icon": "terminal-powershell"
        },
        "Command Prompt": {
            "path": [
                "${env:windir}\\Sysnative\\cmd.exe",
                "${env:windir}\\System32\\cmd.exe"
            ],
            "args": [],
            "icon": "terminal-cmd"
        },
        "Git Bash": {
            "source": "Git Bash"
        },
        "Windows PowerShell": {
            "path": "C:\\WINDOWS\\System32\\WindowsPowerShell\\v1.0\\powershell.exe"
        }
    },
    "files.defaultLanguage": "powershell",
    "terminal.integrated.env.windows": {
        
    },
    "github.copilot.enable": {
        "*": true,
        "plaintext": true,
        "markdown": true,
        "scminput": false
    },
    "github.copilot.nextEditSuggestions.enabled": true,
    "github.copilot.advanced": {},
    "debug.openDebug": "openOnDebugBreak",
    "editor.tabCompletion": "off",
    "powershell.integratedConsole.focusConsoleOnExecute": true,
    "editor.wordSeparators": "`~!@#$%^&*()-=+[{]}\\|;:'\",.<>/?",
    "powershell.buttons.showPanelMovementButtons": false,
    "powershell.codeFolding.showLastLine": true,
    "powershell.sideBar.CommandExplorerVisibility": false,
    "editor.accessibilitySupport": "off",
    "powershell.pester.codeLens": false,
    "git.path": "C:\\Program Files\\Git\\bin\\git.exe",
    "terminal.integrated.fontFamily": "JetBrainsMono Nerd Font",
    "editor.fontFamily": "JetBrainsMono Nerd Font, Consolas, 'Courier New', monospace"
}